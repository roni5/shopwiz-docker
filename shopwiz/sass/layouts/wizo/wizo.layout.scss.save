//$desk: 500px; 
//$wide: 896px;

$grids: 4;
$gutters: 1/4;
//$gutter-styles: split fixed;
//$grids: add-grid(8 at $tab);
//$gutters: add-gutter( 1/8 at $tab);
$grids: add-grid(12 at $desk);
$gutters: add-gutter( 0.25 at $desk);
$grids: add-grid(16 at $wide);
$gutters: add-gutter( 0.15 at $wide);


