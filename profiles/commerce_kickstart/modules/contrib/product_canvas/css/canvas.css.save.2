/* BOTTOM BARS ================================= */
.bar 	{ position:relative; 
          display:block; 
        margin-left: auto;
    margin-right: auto; 
         //width:300px;
     padding:0 20px; 
      }
.bar:before, .bar:after 	{
  content:'';
  height:2px; 
  width:0;
  bottom:1px; 
  position:absolute;
  background:#ff0099; 
  transition:0.2s ease all; 
  -moz-transition:0.2s ease all; 
  -webkit-transition:0.2s ease all;
}
.bar:before {
  left:50%;
}
.bar:after {
  right:50%; 
}

/* active state */
input:focus ~ .bar:before, input:focus ~ .bar:after {
  width:50%;
}


